+    ...     'subcubes': {
+    ...         'Bill': {
+    ...             'subcubes': {
+    ...                 'piano': {'measure': 1},
+    ...                 'trumpet': {'measure': 0},
+    ...             },
+    ...             'measure': 1
+    ...         },
+    ...         'Miles': {
+    ...             'subcubes': {
+    ...                 'piano': {'measure': 0},
+    ...                 'trumpet': {'measure': 1},
+    ...             },
+    ...             'measure': 1
+    ...         },
+    ...         'Thelonious': {
+    ...             'subcubes': {
+    ...                 'piano': {'measure': 1},
+    ...                 'trumpet': {'measure': 0},
+    ...             },
+    ...             'measure': 1
+    ...         },
+    ...         'Freddie': {
+    ...             'subcubes': {
+    ...                 'piano': {'measure': 0},
+    ...                 'trumpet': {'measure': 1},
+    ...             },
+    ...             'measure': 1
