+</head>
+<body>
+<div class="wrapper">
+    <article class="main">
+        {% include "contents/header.html" %}
+        {% if content.page_slug = "home" %}
+        <ul class="bxslider">
+            {% for slide in slideshowcollection %}
+            <li><img src='/{{ slide.url|cut:"contents/" }}' title="{{ slide.alt }}"/></li>
+            {% endfor %}
+            {% endif %}
+        </ul>
+        <div class="clearfix">
+            <div class="main-left">
+                {% block content %}
+                <h1>Search</h1>
+
+                <form method="get" action="." id="hongkiat-form">
+
